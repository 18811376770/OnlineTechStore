<% include ../partials/header %> 

 <div class="container">
     <header class="jumbotron">
         <div class="container">
             <h1>Your Orders </h1>
         </div>
     </header>
    <ul class="list-group">
        <% orders.forEach(function(order){ %>
            <li class="list-item">
                <div> <%= order.time %></div>
                <% var size = order.items.length %>
                <% for (var i = 0; i < size; i++){ %>
                    <div><%= order.items[i] %> </div>
                    <div>  Quantity: <%= order.quantities[i] %> </div>
                    <img style="width: 255px; height: 155px;" src="<%= order.images[i] %>">
                <% } %>
                <div>Total: <%= order.sum %></div>
            </li>
            <br>
        <% }); %>
    </ul>
    <br>
      <a href="/techs">Go Back</a>
</div>

<% include ../partials/footer %>
